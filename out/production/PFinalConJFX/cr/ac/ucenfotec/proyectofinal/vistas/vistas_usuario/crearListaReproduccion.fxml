            <Font size="14.0" />
         </font></Button>
      <Text fill="WHITE" layoutX="700.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Crear lista de reproducción">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <TextField fx:id="fieldBusqueda" layoutX="720.0" layoutY="271.0" prefHeight="29.0" prefWidth="246.0" promptText="Buscar canción" />
      <Button fx:id="btnAgregarCancion" layoutX="572.0" layoutY="776.0" mnemonicParsing="false" onAction="#agregarCancionArray" prefHeight="39.0" prefWidth="198.0" text="Agregar canción a la lista">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Pane prefHeight="1000.0" prefWidth="210.0" style="-fx-background-color: black;">
