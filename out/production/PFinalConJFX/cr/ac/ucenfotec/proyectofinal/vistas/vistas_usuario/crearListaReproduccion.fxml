            <TableColumn fx:id="columnId" editable="false" minWidth="0.0" prefWidth="49.0" text="Id" />
            <TableColumn fx:id="columnNombre" editable="false" minWidth="7.0" prefWidth="238.0" text="Nombre" />
            <TableColumn fx:id="columnArtista" editable="false" prefWidth="173.0" text="Artista" />
            <TableColumn fx:id="columnCompositor" editable="false" prefWidth="201.0" text="Compositor" />
            <TableColumn fx:id="columnFechaLanz" editable="false" prefWidth="170.0" text="Fecha lanzamiento" />
