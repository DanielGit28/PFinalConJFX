      <Button fx:id="btnQuitarCancion" layoutX="916.0" layoutY="776.0" mnemonicParsing="false" onAction="#quitarCancionArray" prefHeight="39.0" prefWidth="198.0" text="Remover canción">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
